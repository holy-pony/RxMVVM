<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ru.nitrobubbles.rxmvvm.MainActivity">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ru.nitrobubbles.rxmvvm.model.ui.BindedTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </ru.nitrobubbles.rxmvvm.model.ui.BindedTextView>

        <ru.nitrobubbles.rxmvvm.model.ui.BindedEditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"></ru.nitrobubbles.rxmvvm.model.ui.BindedEditText>

    </LinearLayout>
</android.support.constraint.ConstraintLayout>
